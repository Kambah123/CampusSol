# Campus SOL Starter - Environment Variables
# Copy this file to .env and fill in your values

# Solana Network Configuration
VITE_SOLANA_NETWORK=devnet
VITE_RPC_ENDPOINT=https://api.devnet.solana.com

# Admin Wallet (for rewards - keep secure!)
# This is the private key of the wallet that will send rewards to users
# Format: Base58 encoded private key
VITE_ADMIN_PRIVATE_KEY=your_admin_wallet_private_key_here

# USDC Mint Address (Devnet)
VITE_USDC_MINT=4zMMC9srt5Ri5X14GAgXhaHii3GnPAEERYPJgZJDncDU

# Verification Address (for Quest 2 - users send micro-tx here)
VITE_VERIFICATION_ADDRESS=your_verification_wallet_address_here

# Telegram Bot Configuration
VITE_BOT_NAME=CampusSolNaijaBot
VITE_BOT_URL=https://t.me/CampusSolNaijaBot

# Metaplex / NFT Configuration
VITE_NFT_COLLECTION_ADDRESS=your_nft_collection_address_here

# Webacy / Security Configuration
VITE_WEBACY_API_KEY=your_webacy_api_key_here

# App Configuration
VITE_APP_NAME=Campus SOL Starter
VITE_APP_URL=https://your-deployed-url.vercel.app
